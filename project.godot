; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="NetTemplate"
run/main_scene="res://ui/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Configuration="*res://utilities/settings/Configuration.cs"
AudioManager="*res://utilities/managers/AudioManager.cs"
VideoManager="*res://utilities/managers/VideoManager.cs"
InputManager="*res://utilities/managers/InputManager.cs"
SceneManager="*res://utilities/managers/SceneManager.cs"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/resizable=false

[dotnet]

project/assembly_name="NetTemplate"

[editor_plugins]

enabled=PackedStringArray("res://addons/logging/plugin.cfg")

[input]

Pause={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":80,"key_label":0,"unicode":112,"echo":false,"script":null)
, Object(InputEventJoypadButton,"resource_local_to_scene":false,"resource_name":"","device":-1,"button_index":6,"pressure":0.0,"pressed":false,"script":null)
]
}

[plugins]

logger/level=0
